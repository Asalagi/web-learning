<%- include("partials/header") -%>

<h1>Compose</h1>
<form action="/compose" method="post">
    <div class="form-group">
        <label for="exampleFormControlInput1">Title</label>
        <input class="form-control" id="exampleFormControlInput1" type="text" name="postTitle" >
      </div>
      <div class="form-group">
        <label for="exampleFormControlTextarea1">Post</label>
        <textarea class="form-control" id="exampleFormControlTextarea1" rows="5" type="text" name="postMessage"></textarea>
      </div>
      <button class="btn btn-primary" type="submit" name="button">Publish</button>
</form>


<%- include("partials/footer") -%>